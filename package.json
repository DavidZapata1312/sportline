{
  "name": "sportline",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --exec tsx src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "seed": "tsx src/seeders/seeder.ts",
    "seed:prod": "node dist/seeders/seeder.js",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build",
    "docker:seed": "docker-compose -f docker-compose.dev.yml run --rm app npm run seed:prod",
    "docker:down": "docker-compose down && docker-compose -f docker-compose.dev.yml down"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.1",
    "@types/pg": "^8.15.5",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "private": true
}
